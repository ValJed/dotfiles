{
  "console Log": {
    "prefix": "cl",
    "body": [
      "console.log('${0}', ${0})"
    ],
    "description": "Console log"
  },

  "console Log Show": {
    "prefix": "cls",
    "body": [
      "console.log('=====> ${1:HERE} <=====')"
    ],
    "description": "Console Log Show"
  },

  "Console Dir": {
    "prefix": "cd",
    "body": [
      "console.dir(${1}, { depth: ${2:0} })"
    ],
    "description": "Console Dir"
  },

 "For Each": {
    "prefix": "fe",
    "body": [
      "${1}.forEach((${2}) => {\n\t${3}\n})"
    ],
    "description": "For Each"
  },

 "For Of": {
    "prefix": "fo",
    "body": [
      "for (const ${1} of ${2}) {\n\t${3}\n}"
    ],
    "description": "For Of"
  },

 "For Await Of": {
    "prefix": "fao",
    "body": [
      "for await (const ${1} of ${2}) {\n\t${3}\n}"
    ],
    "description": "For Await Of"
  },

 "For In": {
    "prefix": "fi",
    "body": [
      "for (const ${1} in ${2}) {\n\t${3}\n}"
    ],
    "description": "For In"
  },

 "Const": {
    "prefix": "c",
    "body": [
      "const ${1} = ${2}"
    ],
    "description": "Const"
  },

 "Let": {
    "prefix": "l",
    "body": [
      "let ${1} = ${2}"
    ],
    "description": "Let"
  },

 "Function": {
    "prefix": "fn",
    "body": [
      "function ${1}(${2}) {\n\t${3}\n}"
    ],
    "description": "Function"
  },

 "Function Arrow": {
    "prefix": "fa",
    "body": [
      "(${1}) => ${2}"
    ],
    "description": "Function Arrow"
  },

  "Method": {
    "prefix": "fm",
    "body": [
      "${1}(${2}) {",
      "\t${3}",
      "}," 
    ],
    "description": "Method"
  },

 "Await": {
    "prefix": "a",
    "body": [
      "await ${1}"
    ],
    "description": "Await"
  },

  "Map": {
    "prefix": "m",
    "body": [
      "${1}.map((${2}) => ${3})"
    ],
    "description": "Map"
  },

  "Filter": {
    "prefix": "f",
    "body": [
      "${1}.filter((${2}) => ${3})"
    ],
    "description": "Filter"
  },

  "Reduce": {
    "prefix": "r",
    "body": [
      "${1}.reduce((${2}, acc) => {\n\t${4}\n}, ${3})"
    ],
    "description": "Reduce"
  },

  "Promise": {
    "prefix": "p",
    "body": [
      "new Promise((resolve, reject) => {\n\t${1}\n})"
    ],
    "description": "Promise"
  },

  "Set Timeout": {
    "prefix": "st",
    "body": [
      "setTimeout(() => {\n\t${2}\n}, ${1})"
    ],
    "description": "Set Timeout"
  },

  "Set Interval": {
    "prefix": "si",
    "body": [
      "setInterval(() => {\n\t${2}\n}, ${1})"
    ],
    "description": "Set Interval"
  },

  "Module Export": {
    "prefix": "me",
    "body": [
      "module.exports = ${1}"
    ],
    "description": "Module Export"
  },

  "Export Default": {
    "prefix": "ed",
    "body": [
      "export default ${1}"
    ],
    "description": "Export Default"
  },

  "Export Const": {
    "prefix": "ec",
    "body": [
      "export const ${1} = ${2}"
    ],
    "description": "Export Const"
  },

  "Try Catch": {
    "prefix": "tc",
    "body": [
      "try {\n\t${1}\n} catch (err) {\n\t${2}\n}"
    ],
    "description": "Try Catch"
  },

  "Try Catch Finally": {
    "prefix": "tcf",
    "body": [
      "try {\n\t${1}\n} catch (err) {\n\t${2}\n} finally {\n\t${3}\n}"
    ],
    "description": "Try Catch Finally"
  },

  "If": {
    "prefix": "if",
    "body": [
      "if (${1}) {\n\t${2}\n}"
    ],
    "description": "If"
  },

  "If Else": {
    "prefix": "ife",
    "body": [
      "if (${1}) {\n\t${2}\n} else {\n\t${3}\n}"
    ],
    "description": "If Else"
  },

  "Apos Field": {
    "prefix": "aposField",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: '${3}',",
      "$\t{4:required: true,}",
      "},"
    ],
    "description": "Apos Field"
  },

  "Apos String Field": {
    "prefix": "aposString",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'string',",
      "\t${4:required: true,}",
      "},"
    ],
    "description": "Apos String Field"
  },

  "Apos Boolean Field": {
    "prefix": "aposBool",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'boolean',",
      "\t${4:required: true,}",
      "},"
    ],
    "description": "Apos Boolean Field"
  },

  "Apos Select Field": {
    "prefix": "aposSelect",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'select',",
      "\tchoices: [",
      "\t\t${3}",
      "\t],",
      "\t${4:required: true,}",
      "},"
    ],
    "description": "Apos Select Field"
  },

  "Apos Checkboxes Field": {
    "prefix": "aposCheckbox",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'checkboxes',",
      "\tchoices: [",
      "\t\t${3}",
      "\t],",
      "\t${4:required: true,}",
      "},"
    ],
    "description": "Apos Checkboxes Field"
  },

  "Apos Array Field": {
    "prefix": "aposArray",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'array',",
      "\tfields: {",
      "\t\tadd: {",
      "\t\t\t${3}",
      "\t\t},",
      "\t},",
      "\t${4:required: true,}",
      "},"
    ],

    "description": "Apos Array Field"
  },

  "Apos Object Field": {
    "prefix": "aposObject",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'object',",
      "\tfields: {",
      "\t\tadd: {",
      "\t\t\t${3}",
      "\t\t},",
      "\t},",
      "},"
    ],
    "description": "Apos Object Field"
  },

  "Apos Relationship Field": {
    "prefix": "aposRelationship",
    "body": [
      "_${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'relationship',",
      "\twithType: '${3}',",
      "\t${4:required: true,}",
      "},"
    ],
    "description": "Apos Relationship Field"
  },

  "Apos Area Field": {
    "prefix": "aposArea",
    "body": [
      "${1}: {",
      "\tlabel: '${2}',",
      "\ttype: 'area',",
      "\toptions: {",
      "\t\twidgets: {",
      "\t\t\t${3}",
      "\t\t}",
      "\t},",
      "\t${4:required: true,}",
      "},"
    ],
    "description": "Apos Area Field"
  },

  "Apos Choice": {
    "prefix": "aposChoice",
    "body": [
      "{",
      "\tlabel: '${1}',",
      "\tvalue: ${2},",
      "}"
    ],
    "description": "Apos Choice"
  }
}
