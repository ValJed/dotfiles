{
  "Ref": {
    "prefix": "ref",
    "body": [
      "const ${1} = ref(${2})"
    ],
    "description": "Ref"
  },

  "Define Props": {
    "prefix": "dp",
    "body": [
      "const ${1:props} = defineProps({\n\t${2}\n})"
    ],
    "description": "Define Props"
  },

  "Define Meta": {
    "prefix": "dm",
    "body": [
      "definePageMeta({\n\t${1}\n})"
    ],
    "description": "Define Meta"
  },

  "Prop": {
    "prefix": "prop",
    "body": [
      "${1}: {\n\ttype: ${2:String},\n\trequired: ${3:true}\n}"
    ],
    "description": "Prop"
  },

  "On Mounted": {
    "prefix": "om",
    "body": [
      "onMounted(() => {\n\t${1}\n})"
    ],
    "description": "On Mounted"
  },

  "Component": {
    "prefix": "cc",
    "body": [
      "<template>\n\t${1}\n</template>\n\n<script setup>\n\t${2}\n</script>\n\n<style lang=\"scss>\"\n${3}\n</style>"
    ],
    "description": "Component"
  }
}
