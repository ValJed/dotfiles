{
  "For": {
    "prefix": "f",
    "body": [
      "{% for ${1} in ${2} $}",
      "\t${3}",
      "{% endfor %}"
    ],
    "description": "For"
  },

  "Interpolate": {
    "prefix": "i",
    "body": [
      "{{ ${1} }}"
    ],
    "description": "Interpolate"
  },

  "If": {
    "prefix": "if",
    "body": [
      "{% if ${1} %}\n\t${2}\n{% endif %}"
    ],
    "description": "If"
  },

  "If Else": {
    "prefix": "ie",
    "body": [
      "{% if ${1} %}\n\t${2}\n{% else %}\n\t${3}\n${% endif %}"
    ],
    "description": "If Else"
  },

  "Elif": {
    "prefix": "elif",
    "body": [
      "{% if ${1} %}\n\t${2}\n{% elif ${3} %}\n\t${4}\n{% endif %}"
    ],
    "description": "Elif"
  },

  "Set": {
    "prefix": "set",
    "body": [
      "{% set ${1} = ${2} %}"
    ],
    "description": "Set"
  },

  "Apos Image": {
    "prefix": "aposImg",
    "body": [
      "{% set ${1:img} = apos.image.first(${2}) %}"
    ],
    "description": "Apos Image"
  },

  "Apos Attachment": {
    "prefix": "aposAttachment",
    "body": [
      "{% set ${1:imgUrl} = apos.attachment.url(${2}) %}"
    ],
    "description": "Apos Attachment"
  },

  "Apos Log": {
    "prefix": "cl",
    "body": [
      "{{ apos.log('${1}', ${1}) }}"
    ],
    "description": "Apos Log"
  }
}
